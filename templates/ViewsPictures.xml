<?xml version="1.0" encoding="UTF-8"?>
<includes>
	<include name="PictureWrapView">
		<control type="group">
			<visible>Control.IsVisible(510)</visible>
			<include>VisibleFadeEffect</include>
			<left>{{ScaleX('0')}}</left>
			<top>{{ScaleY('440')}}</top>
			<control type="wraplist" id="510">
				<depth>DepthContent+</depth>
				<left>{{ScaleX('-25')}}</left>
				<top>{{ScaleY('5')}}</top>
				<width>{{ScaleX('1330')}}</width>
				<height>{{ScaleY('250')}}</height>
				<onleft>510</onleft>
				<onright>510</onright>
				<onup>2</onup>
				<ondown>60</ondown>
				<viewtype label="31032">wrap</viewtype>
				<pagecontrol>60</pagecontrol>
				<scrolltime>200</scrolltime>
				<focusposition>3</focusposition>
				<orientation>horizontal</orientation>
				<preloaditems>4</preloaditems>
				<itemlayout height="{{ScaleY('200')}}" width="{{ScaleX('190')}}">
					<control type="image">
						<left>{{ScaleX('15')}}</left>
						<top>{{ScaleY('40')}}</top>
						<width>{{ScaleX('160')}}</width>
						<height>{{ScaleY('160')}}</height>
						<aspectratio>stretch</aspectratio>
						<bordertexture border="{{ScaleBorder('10')}}">ThumbShadow.png</bordertexture>
						<bordersize>{{ScaleBorder('8')}}</bordersize>
						<fadetime>200</fadetime>
						<texture border="{{ScaleBorder('2')}}">ThumbBG.png</texture>
					</control>
					<control type="image">
						<left>{{ScaleX('25')}}</left>
						<top>{{ScaleY('50')}}</top>
						<width>{{ScaleX('140')}}</width>
						<height>{{ScaleY('140')}}</height>
						<aspectratio>keep</aspectratio>
						<texture background="true">$INFO[ListItem.Icon]</texture>
					</control>
				</itemlayout>
				<focusedlayout height="{{ScaleY('200')}}" width="{{ScaleX('190')}}">
					<control type="image">
						<depth>DepthContentPopout</depth>
						<left>{{ScaleX('15')}}</left>
						<top>{{ScaleY('40')}}</top>
						<width>{{ScaleX('160')}}</width>
						<height>{{ScaleY('160')}}</height>
						<aspectratio>stretch</aspectratio>
						<bordertexture border="{{ScaleBorder('10')}}">ThumbShadow.png</bordertexture>
						<bordersize>{{ScaleBorder('8')}}</bordersize>
						<fadetime>200</fadetime>
						<texture border="{{ScaleBorder('2')}}">ThumbBG.png</texture>
						<animation reversible="false" effect="zoom" start="{{ScaleBorder2('15,40,160,160')}}" end="{{ScaleBorder2('-10,-5,210,210')}}" time="150">focus</animation>
						<animation reversible="false" effect="zoom" end="{{ScaleBorder2('15,40,160,160')}}" start="{{ScaleBorder2('-10,-5,210,210')}}" time="150">unfocus</animation>
					</control>
					<control type="image">
						<depth>DepthContentPopout</depth>
						<left>{{ScaleX('25')}}</left>
						<top>{{ScaleY('50')}}</top>
						<width>{{ScaleX('140')}}</width>
						<height>{{ScaleY('140')}}</height>
						<aspectratio>keep</aspectratio>
						<texture background="true">$INFO[ListItem.Icon]</texture>
						<animation reversible="false" effect="zoom" start="{{ScaleBorder2('25,50,140,140')}}" end="{{ScaleBorder2('5,10,180,180')}}" time="150">focus</animation>
						<animation reversible="false" effect="zoom" end="{{ScaleBorder2('25,50,140,140')}}" start="{{ScaleBorder2('5,10,180,180')}}" time="150">unfocus</animation>
					</control>
				</focusedlayout>
			</control>
			<control type="scrollbar" id="60">
				<left>{{ScaleX('310')}}</left>
				<top>{{ScaleY('200')}}</top>
				<width>{{ScaleX('660')}}</width>
				<height>{{ScaleY('25')}}</height>
				<texturesliderbackground border="{{ScaleBorder('14,0,14,0')}}">ScrollBarH.png</texturesliderbackground>
				<texturesliderbar border="{{ScaleBorder('14,0,14,0')}}">ScrollBarH_bar.png</texturesliderbar>
				<texturesliderbarfocus border="{{ScaleBorder('14,0,14,0')}}">ScrollBarH_bar_focus.png</texturesliderbarfocus>
				<textureslidernib>ScrollBarNib.png</textureslidernib>
				<textureslidernibfocus>ScrollBarNib.png</textureslidernibfocus>
				<onup>510</onup>
				<ondown>2</ondown>
				<showonepage>false</showonepage>
				<orientation>horizontal</orientation>
				<visible>Control.IsVisible(510)</visible>
			</control>
		</control>
	</include>
	<include name="PictureThumbView">
		<control type="group">
			<visible>Control.IsVisible(514)</visible>
			<control type="panel" id="514">
				<left>{{ScaleX('60')}}</left>
				<top>{{ScaleY('75')}}</top>
				<width>{{ScaleX('432')}}</width>
				<height>{{ScaleY('576')}}</height>
				<onleft>2</onleft>
				<onright>60</onright>
				<onup>514</onup>
				<ondown>514</ondown>
				<viewtype label="$LOCALIZE[31031]">list</viewtype>
				<pagecontrol>60</pagecontrol>
				<scrolltime>200</scrolltime>
				<preloaditems>2</preloaditems>
				<itemlayout height="{{ScaleY('144')}}" width="{{ScaleX('144')}}">
					<control type="image">
						<left>{{ScaleX('0')}}</left>
						<top>{{ScaleY('0')}}</top>
						<width>{{ScaleX('144')}}</width>
						<height>{{ScaleY('144')}}</height>
						<aspectratio>stretch</aspectratio>
						<bordertexture border="{{ScaleBorder('10')}}">ThumbShadow.png</bordertexture>
						<bordersize>{{ScaleBorder('8')}}</bordersize>
						<fadetime>200</fadetime>
						<texture border="{{ScaleBorder('2')}}">ThumbBG.png</texture>
					</control>
					<control type="image">
						<left>{{ScaleX('10')}}</left>
						<top>{{ScaleY('10')}}</top>
						<width>{{ScaleX('124')}}</width>
						<height>{{ScaleY('124')}}</height>
						<aspectratio>keep</aspectratio>
						<texture background="true">$INFO[ListItem.Icon]</texture>
					</control>
				</itemlayout>
				<focusedlayout height="{{ScaleY('144')}}" width="{{ScaleX('144')}}">
					<control type="image">
						<left>{{ScaleX('0')}}</left>
						<top>{{ScaleY('0')}}</top>
						<width>{{ScaleX('144')}}</width>
						<height>{{ScaleY('144')}}</height>
						<aspectratio>stretch</aspectratio>
						<bordertexture border="{{ScaleBorder('10')}}">folder-Focus.png</bordertexture>
						<bordersize>{{ScaleBorder('8')}}</bordersize>
						<fadetime>200</fadetime>
						<texture border="{{ScaleBorder('2')}}">ThumbBG.png</texture>
					</control>
					<control type="image">
						<left>{{ScaleX('10')}}</left>
						<top>{{ScaleY('10')}}</top>
						<width>{{ScaleX('124')}}</width>
						<height>{{ScaleY('124')}}</height>
						<aspectratio>keep</aspectratio>
						<texture background="true">$INFO[ListItem.Icon]</texture>
					</control>
				</focusedlayout>
			</control>
			<control type="scrollbar" id="60">
				<left>{{ScaleX('500')}}</left>
				<top>{{ScaleY('70')}}</top>
				<width>{{ScaleX('25')}}</width>
				<height>{{ScaleY('576')}}</height>
				<texturesliderbackground border="{{ScaleBorder('0,14,0,14')}}">ScrollBarV.png</texturesliderbackground>
				<texturesliderbar border="{{ScaleBorder('0,14,0,14')}}">ScrollBarV_bar.png</texturesliderbar>
				<texturesliderbarfocus border="{{ScaleBorder('0,14,0,14')}}">ScrollBarV_bar_focus.png</texturesliderbarfocus>
				<textureslidernib>ScrollBarNib.png</textureslidernib>
				<textureslidernibfocus>ScrollBarNib.png</textureslidernibfocus>
				<onleft>514</onleft>
				<onright>2</onright>
				<showonepage>false</showonepage>
				<orientation>vertical</orientation>
				<visible>Control.IsVisible(514)</visible>
			</control>
			<control type="group">
				<left>{{ScaleX('570')}}</left>
				<top>{{ScaleY('80')}}</top>
				<control type="label">
					<description>Title txt</description>
					<left>{{ScaleX('0')}}</left>
					<top>{{ScaleY('0')}}</top>
					<width>{{ScaleX('640')}}</width>
					<height>{{ScaleY('15')}}</height>
					<label>$INFO[ListItem.Label]</label>
					<align>center</align>
					<aligny>center</aligny>
					<font>font24_title</font>
					<textcolor>white</textcolor>
					<shadowcolor>black</shadowcolor>
				</control>
				<control type="image">
					<left>{{ScaleX('0')}}</left>
					<top>{{ScaleY('30')}}</top>
					<width>{{ScaleX('640')}}</width>
					<height>{{ScaleY('470')}}</height>
					<texture background="true">$INFO[ListItem.FilenameAndPath]</texture>
					<fadetime>600</fadetime>
					<aspectratio>keep</aspectratio>
					<bordertexture border="{{ScaleBorder('8')}}">ThumbShadow.png</bordertexture>
					<bordersize>{{ScaleBorder('9')}}</bordersize>
				</control>
				<control type="label">
					<description>Date time txt</description>
					<left>{{ScaleX('0')}}</left>
					<top>{{ScaleY('510')}}</top>
					<width>{{ScaleX('640')}}</width>
					<height>{{ScaleY('15')}}</height>
					<label>$INFO[ListItem.PictureDateTime,[COLOR=blue]$LOCALIZE[31326][/COLOR] - ]</label>
					<align>left</align>
					<aligny>center</aligny>
					<font>font13</font>
					<textcolor>white</textcolor>
					<shadowcolor>black</shadowcolor>
				</control>
				<control type="label">
					<description>Resolution txt</description>
					<left>{{ScaleX('0')}}</left>
					<top>{{ScaleY('535')}}</top>
					<width>{{ScaleX('640')}}</width>
					<height>{{ScaleY('15')}}</height>
					<label>$INFO[ListItem.PictureResolution,[COLOR=blue]$LOCALIZE[31327][/COLOR] - ]</label>
					<align>left</align>
					<aligny>center</aligny>
					<font>font13</font>
					<textcolor>white</textcolor>
					<shadowcolor>black</shadowcolor>
				</control>
			</control>
		</control>
	</include>
</includes>
