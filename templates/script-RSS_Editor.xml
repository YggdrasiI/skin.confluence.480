<?xml version="1.0" encoding="UTF-8"?>
<window>
	<defaultcontrol>10</defaultcontrol>
	<coordinates>
		<left>{{ScaleX('240')}}</left>
		<top>{{ScaleY('140')}}</top>
	</coordinates>
	<include>dialogeffect</include>
	<controls>
		<include content="DialogBackgroundCommons">
			<param name="DialogBackgroundWidth" value="800" />
			<param name="DialogBackgroundHeight" value="420" />
			<param name="DialogHeaderWidth" value="720" />
			<param name="DialogHeaderLabel" value="" />
			<param name="DialogHeaderId" value="2" />
			<param name="CloseButtonLeft" value="710" />
			<param name="CloseButtonNav" value="10" />
		</include>
		<control type="label" id="4">
			<description>List label</description>
			<left>{{ScaleX('20')}}</left>
			<top>{{ScaleY('65')}}</top>
			<width>{{ScaleX('760')}}</width>
			<height>{{ScaleY('30')}}</height>
			<align>center</align>
			<aligny>center</aligny>
			<font>font12</font>
			<textcolor>blue</textcolor>
			<shadowcolor>black</shadowcolor>
		</control>
		<control type="list" id="10">
			<left>{{ScaleX('30')}}</left>
			<top>{{ScaleY('105')}}</top>
			<width>{{ScaleX('520')}}</width>
			<height>{{ScaleY('225')}}</height>
			<onup>9001</onup>
			<onleft>9000</onleft>
			<onright>60</onright>
			<ondown>9001</ondown>
			<pagecontrol>60</pagecontrol>
			<scrolltime>200</scrolltime>
			<itemlayout height="{{ScaleY('45')}}">
				<control type="image">
					<left>{{ScaleX('0')}}</left>
					<top>{{ScaleY('0')}}</top>
					<width>{{ScaleX('520')}}</width>
					<height>{{ScaleY('40')}}</height>
					<texture border="{{ScaleBorder('5')}}">button-nofocus.png</texture>
				</control>
				<control type="label">
					<left>{{ScaleX('10')}}</left>
					<top>{{ScaleY('0')}}</top>
					<width>{{ScaleX('500')}}</width>
					<height>{{ScaleY('40')}}</height>
					<font>font13</font>
					<align>left</align>
					<aligny>center</aligny>
					<textcolor>grey2</textcolor>
					<selectedcolor>selected</selectedcolor>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			<focusedlayout height="{{ScaleY('45')}}">
				<control type="image">
					<left>{{ScaleX('0')}}</left>
					<top>{{ScaleY('0')}}</top>
					<width>{{ScaleX('520')}}</width>
					<height>{{ScaleY('40')}}</height>
					<visible>!Control.HasFocus(10)</visible>
					<texture border="{{ScaleBorder('5')}}">button-nofocus.png</texture>
				</control>
				<control type="image">
					<left>{{ScaleX('0')}}</left>
					<top>{{ScaleY('0')}}</top>
					<width>{{ScaleX('520')}}</width>
					<height>{{ScaleY('40')}}</height>
					<visible>Control.hasfocus(14) | Control.HasFocus(10)</visible>
					<texture border="{{ScaleBorder('5')}}">button-focus2.png</texture>
				</control>
				<control type="label">
					<left>{{ScaleX('10')}}</left>
					<top>{{ScaleY('0')}}</top>
					<width>{{ScaleX('500')}}</width>
					<height>{{ScaleY('40')}}</height>
					<font>font13</font>
					<align>left</align>
					<aligny>center</aligny>
					<selectedcolor>selected</selectedcolor>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
		</control>
		<control type="scrollbar" id="60">
			<left>{{ScaleX('552')}}</left>
			<top>{{ScaleY('105')}}</top>
			<width>{{ScaleX('25')}}</width>
			<height>{{ScaleY('225')}}</height>
			<texturesliderbackground border="{{ScaleBorder('0,14,0,14')}}">ScrollBarV.png</texturesliderbackground>
			<texturesliderbar border="{{ScaleBorder('0,14,0,14')}}">ScrollBarV_bar.png</texturesliderbar>
			<texturesliderbarfocus border="{{ScaleBorder('0,14,0,14')}}">ScrollBarV_bar_focus.png</texturesliderbarfocus>
			<textureslidernib>ScrollBarNib.png</textureslidernib>
			<textureslidernibfocus>ScrollBarNib.png</textureslidernibfocus>
			<onleft>10</onleft>
			<onright>9000</onright>
			<showonepage>false</showonepage>
			<orientation>vertical</orientation>
		</control>
		<control type="label">
			<description>number of files/pages in list text label</description>
			<top>{{ScaleY('240')}}</top>
			<left>{{ScaleX('480')}}</left>
			<width>{{ScaleX('275')}}</width>
			<height>{{ScaleY('35')}}</height>
			<font>font12</font>
			<align>right</align>
			<aligny>center</aligny>
			<textcolor>blue</textcolor>
			<label>[COLOR=FF12B2E7]$INFO[Container(10).NumItems][/COLOR] $ADDON[script.rss.editor 32002] - $ADDON[script.rss.editor 32003] [COLOR=FF12B2E7]$INFO[Container(10).CurrentPage]/$INFO[Container(10).NumPages][/COLOR]</label>
		</control>
		<control type="group" id="9000">
			<left>{{ScaleX('580')}}</left>
			<top>{{ScaleY('105')}}</top>
			<control type="button" id="11">
				<description>Change Set Button</description>
				<left>{{ScaleX('0')}}</left>
				<top>{{ScaleY('0')}}</top>
				<width>{{ScaleX('200')}}</width>
				<height>{{ScaleY('40')}}</height>
				<font>font12_title</font>
				<align>center</align>
				<aligny>center</aligny>
				<onup>18</onup>
				<onleft>60</onleft>
				<onright>10</onright>
				<ondown>13</ondown>
			</control>
			<control type="button" id="13">
				<description>Add Button</description>
				<left>{{ScaleX('0')}}</left>
				<top>{{ScaleY('45')}}</top>
				<width>{{ScaleX('200')}}</width>
				<height>{{ScaleY('40')}}</height>
				<font>font12_title</font>
				<label>15019</label>
				<align>center</align>
				<aligny>center</aligny>
				<onup>11</onup>
				<onleft>60</onleft>
				<onright>10</onright>
				<ondown>14</ondown>
			</control>
			<control type="button" id="14">
				<description>Remove Button</description>
				<left>{{ScaleX('0')}}</left>
				<top>{{ScaleY('90')}}</top>
				<width>{{ScaleX('200')}}</width>
				<height>{{ScaleY('40')}}</height>
				<font>font12_title</font>
				<label>1210</label>
				<align>center</align>
				<aligny>center</aligny>
				<onup>13</onup>
				<onleft>60</onleft>
				<onright>10</onright>
				<ondown>9001</ondown>
			</control>
		</control>
		<control type="group" id="9001">
			<control type="button" id="18">
				<description>Ok Button</description>
				<left>{{ScaleX('200')}}</left>
				<top>{{ScaleY('355')}}</top>
				<width>{{ScaleX('200')}}</width>
				<height>{{ScaleY('40')}}</height>
				<align>center</align>
				<aligny>center</aligny>
				<label>186</label>
				<font>font12_title</font>
				<onup>9000</onup>
				<onleft>20</onleft>
				<onright>19</onright>
				<ondown>10</ondown>
			</control>
			<control type="button" id="19">
				<description>Cancel Button</description>
				<left>{{ScaleX('410')}}</left>
				<top>{{ScaleY('355')}}</top>
				<width>{{ScaleX('200')}}</width>
				<height>{{ScaleY('40')}}</height>
				<align>center</align>
				<aligny>center</aligny>
				<label>222</label>
				<font>font12_title</font>
				<onup>9000</onup>
				<onleft>18</onleft>
				<onright>20</onright>
				<ondown>10</ondown>
			</control>
		</control>
	</controls>
</window>
