		</control>
		<include>MainWindowMouseButtons</include>
		<include content="CommonWindowHeader">
			<param name="Icon" value="icon_system" />
			<param name="Label" value="$LOCALIZE[5]" />
			<param name="ShowLabel2" value="true" />
		</include>
