			</control>
			<control type="scrollbar" id="60">
				<left>{{ScaleX({{ 1060 + {SettingPanelExtraWidthL} +{SettingPanelExtraWidthR} }})}}</left>
				<top>{{ScaleY('60')}}</top>
				<width>{{ScaleX('25')}}</width>
